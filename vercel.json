{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "**/*.ts"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts"
        }
    ]
}